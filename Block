import java.awt.Image;

public class Block {
    public int x, y, width, height;
    public Image image;
    
    public Block(Image image, int x, int y, int width, int height) {
        this.image = image;
        this.x = x;
        this.y = y;
        this.width = width;
        this.height = height;
    }
}
